# Environment configuration for ParaBank test automation
# Copy this file to .env and update values as needed

# Base URL for ParaBank application
BASE_URL=https://parabank.parasoft.com

# API base URL (optional, defaults to BASE_URL + services path)
API_BASE_URL=https://parabank.parasoft.com/parabank/services/bank

# Environment (development, staging, production)
ENV=development

# Test configuration
SCREENSHOT_ON_FAILURE=true
TRACE_ON_RETRY=true
DEFAULT_TIMEOUT=30000
NAVIGATION_TIMEOUT=60000
RETRY_ATTEMPTS=2
